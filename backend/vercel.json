{
    "version": 2,
    "builds": [
        {
        "src": "index.py",
        "use": "@vercel/python",
        "config": { 
            "runtime": "python3.10",
            "pythonVersion": "3.10"
        }
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "index.py"
        }
    ],
    "functions": {
        "**/*.py": {
            "runtime": "python3.10"
        }
    }
}